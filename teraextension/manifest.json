{
    "name": "Tera Link Extractor",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "webNavigation",
        "storage",
        "scripting"
      ],
      "background": {
        "service_worker": "background.js"
      },
      "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ],
      "host_permissions": [
        "http://*/*",
        "https://*/*"
      ]
    }